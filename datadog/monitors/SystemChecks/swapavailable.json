{
	"name": "Swap Available (%)",
	"type": "metric alert",
	"query": "max(last_5m):avg:system.swap.pct_free{*} < 0.15",
	"message": "Alert triggered if swap memory available falls below 15%",
	"tags": [
		"system"
	],
	"options": {
		"notify_audit": false,
		"locked": false,
		"timeout_h": 0,
		"new_host_delay": 300,
		"require_full_window": false,
		"notify_no_data": false,
		"renotify_interval": "0",
		"escalation_message": "",
		"no_data_timeframe": null,
		"include_tags": true,
		"thresholds": {
			"critical": 0.15
		}
	}
}
