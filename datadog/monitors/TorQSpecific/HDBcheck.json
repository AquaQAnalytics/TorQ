{
	"name": "Data Check - hdb1",
	"type": "query alert",
	"query": "min(last_1m):default_zero(avg:process_hdb1_check{*}) < 1",
	"message": "Checks that yesterday's data saved to disk and has been loaded into the process.",
	"tags": [
		"datacheck"
	],
	"options": {
		"notify_audit": false,
		"locked": false,
		"timeout_h": 0,
		"new_host_delay": 300,
		"require_full_window": false,
		"notify_no_data": false,
		"renotify_interval": "0",
		"escalation_message": "",
		"no_data_timeframe": null,
		"include_tags": true,
		"thresholds": {
			"critical": 1
		}
	}
}
